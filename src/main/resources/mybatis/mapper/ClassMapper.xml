<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scnu.peexamsystem.dao.classes.ClassDao">

    <select id="findAllByInstituteNoAndGrade" resultType="Class">
        select * from class where institute_no = #{instituteNo} and grade = #{grade};
    </select>

</mapper>