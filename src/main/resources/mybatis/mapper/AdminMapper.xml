<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scnu.peexamsystem.dao.admin.AdminDao">

    <select id="findAdminByAdminIDAndPassword" resultType="Admin">
        select * from admin where admin_id = #{adminID} and password = #{password}
    </select>

    <select id="findAdminByAdminID" resultType="Admin">
        select * from admin where admin_id = #{adminID};
    </select>
</mapper>